<?xml version="1.0" encoding="UTF-8"?>
<svg version="1.2" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 3508 295 "  width="3508" height="295">
  <svg viewBox="0 0 3508 295">

    <image id="banner"
           href="{{ banner }}"
           width="3508"
           y="-400"
           preserveAspectRatio="xMidYMid slice"
           clip-path="url(#circleView)"
    ></image>
  </svg>
  <image
          id="logo"
          href="{{ logo }}"
          height="295"
          preserveAspectRatio="xMidYMid slice"
          clip-path="url(#circleView)"
  ></image>
  <image
          id="icon"
          href="{{ icon }}"
          x="3200"
          y="35"
          transform="rotate(90, 3200, 35)"
          width="233" height="295"
  ></image>
  <style>
    tspan { white-space:pre }
    .t0 { font-size: 72px;fill: #ffffff;font-family: "Gill  Sans " }
  </style>
  <text
          x="3400"
          y="285" id="text" style="paint-order:stroke fill markers;stroke: #000000;stroke-width: 10;stroke-linejoin: round;" transform="rotate(270, 3400, 285)">
    <tspan class="t0">{{ set_name }}</tspan>
  </text>
</svg>
